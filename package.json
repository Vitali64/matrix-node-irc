{
  "name": "matrix-org-irc",
  "description": "An IRC client library for node, written in Typescript.",
  "version": "1.2.1",
  "author": "Matrix.org (original fork from Martyn Smith <martyn@dollyfish.net.nz>)",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc --project tsconfig.json",
    "lint": "eslint -c .eslintrc --max-warnings 0 'src/**/*.ts'",
    "test": "tap ./test/test-*.js"
  },
  "contributors": [
    "node-irc contributors"
  ],
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "http://github.com/matrix-org/node-irc"
  },
  "bugs": {
    "url": "http://github.com/matrix-org/node-irc/issues"
  },
  "main": "lib/index",
  "engines": {
    "node": ">=12.0.0"
  },
  "license": "GPL-3.0",
  "dependencies": {
    "chardet": "^1.3.0",
    "iconv-lite": "^0.6.2",
    "irc-colors": "^1.5.0",
    "utf-8-validate": "^5.0.5"
  },
  "devDependencies": {
    "@types/node": "^14",
    "@types/utf-8-validate": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.26.0",
    "tap": "^15.0.9",
    "tape": "^5.2.2",
    "typescript": "^4.2.4"
  },
  "tap": {
    "check-coverage": false,
    "jobs": 1
  }
